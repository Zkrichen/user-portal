pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                withMaven(maven :'maven-3.6.0') 
                sh 'mvn clean compile' 
            }
        }
        stage('Test') { 
            steps {
                sh 'mvn test' 
            }
        }
        stage('Deploy') { 
            steps {
                sh 'mvn deploy' 
            }
        }
    }
}